import pandas as pd

df = pd.read_csv('combined_data_cleaned.csv', delimiter=';', low_memory=False)

def clean_and_convert_to_float(s):
    if isinstance(s, str):
        if s.endswith('-'):
            s = '-' + s[:-1]
        try:
            return float(s)
        except ValueError:
            return None
    else:
        return s

df['Rozpocet'] = df['Rozpocet'].apply(clean_and_convert_to_float)
print(df)
